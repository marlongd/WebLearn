@model IEnumerable<Vidly_authenticate.Models.Movie>
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Index</h2>

@if (!Model.Any())
{
    <h2>We don't have any movies</h2>
}
<ul>

    @foreach (var movie in Model)
    {
        <li class="nav-item">@Html.ActionLink(movie.Name, "Details", "Movie", new { id = movie.Id }, null)</li>
    }
</ul>