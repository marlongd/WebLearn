@model Vidly_authenticate.Models.Customer
@{
    ViewBag.Title = "Details";
}

<h2>@Model.Name</h2>
<ul>
        <li>Membership Type: @Model.MembershipType.Name</li>
        @if (!string.IsNullOrEmpty(Model.Birthdate))
        {
            
            <li>BirthDate: @Model.Birthdate</li>
        }

    
</ul>

